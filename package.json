  "license": "ISC",
  "bugs": {
    "url": "https://github.com/omelandah/interview-assignment/issues"
  },
  "homepage": "https://github.com/omelandah/interview-assignment#readme",
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mysql2": "^3.15.2",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "jest": "^30.2.0",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3",
    "sequelize-cli": "^6.6.3"
  }
}
