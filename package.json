{
  "name": "interview-assignment",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "dev": "nodemon --exec ts-node src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "start": "node dist/app.js",
    "migrate": "ts-node -O '{\"module\":\"CommonJS\"}' node_modules/.bin/sequelize-cli db:migrate",
    "migrate:undo": "ts-node -O '{\"module\":\"CommonJS\"}' node_modules/.bin/sequelize-cli db:migrate:undo",
    "migrate:undo:all": "ts-node -O '{\"module\":\"CommonJS\"}' node_modules/.bin/sequelize-cli db:migrate:undo:all",
    "seed": "ts-node -O '{\"module\":\"CommonJS\"}' node_modules/.bin/sequelize-cli db:seed:all",
    "seed:undo": "ts-node -O '{\"module\":\"CommonJS\"}' node_modules/.bin/sequelize-cli db:seed:undo",
    "seed:undo:all": "ts-node -O '{\"module\":\"CommonJS\"}' node_modules/.bin/sequelize-cli db:seed:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omelandah/interview-assignment.git"
  },
